entity AttendanceExcelUpload {
	file AnyBlob required,
	type AttendanceType required
}

entity Attendance {
	employeeId String required,
    attendanceDate LocalDate required,
    inTime Instant,
    outTime Instant
}

enum AttendanceType {
	FINGER, FACE
}

relationship ManyToOne {
	Attendance{attendanceExcelUploads} to AttendanceExcelUpload
}

paginate * with infinite-scroll
paginate * with pagination
dto * with mapstruct
filter all
service all with serviceImpl
