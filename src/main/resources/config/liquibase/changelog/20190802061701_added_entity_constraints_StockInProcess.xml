<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity StockInProcess.
    -->
    <changeSet id="20190802061701-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="item_categories_id"
                                 baseTableName="stock_in_process"
                                 constraintName="fk_stock_in_process_item_categories_id"
                                 referencedColumnNames="id"
                                 referencedTableName="item_category"/>

        <addForeignKeyConstraint baseColumnNames="item_sub_categories_id"
                                 baseTableName="stock_in_process"
                                 constraintName="fk_stock_in_process_item_sub_categories_id"
                                 referencedColumnNames="id"
                                 referencedTableName="item_sub_category"/>

        <addForeignKeyConstraint baseColumnNames="inventory_locations_id"
                                 baseTableName="stock_in_process"
                                 constraintName="fk_stock_in_process_inventory_locations_id"
                                 referencedColumnNames="id"
                                 referencedTableName="inventory_location"/>

        <addForeignKeyConstraint baseColumnNames="inventory_sub_locations_id"
                                 baseTableName="stock_in_process"
                                 constraintName="fk_stock_in_process_inventory_sub_locations_id"
                                 referencedColumnNames="id"
                                 referencedTableName="inventory_sub_location"/>

        <addForeignKeyConstraint baseColumnNames="manufacturers_id"
                                 baseTableName="stock_in_process"
                                 constraintName="fk_stock_in_process_manufacturers_id"
                                 referencedColumnNames="id"
                                 referencedTableName="manufacturer"/>

    </changeSet>
</databaseChangeLog>
